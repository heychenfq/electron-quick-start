{
  "name": "electron-quick-start",
  "version": "1.0.3",
  "description": "practice electron app",
  "author": "chenfuqiang<heychenfq.foxmail.com>",
  "scripts": {
    "clean": "rimraf dist && rimraf output",
    "start:renderer": "vite --config config/vite.config.ts",
    "build:renderer": "vite build --config config/vite.config.ts",
    "preview:renderer": "vite preview --config config/vite.config.ts",
    "start": "node ./scripts/dev",
    "build": "pnpm clean && node ./scripts/build && pnpm build:renderer",
    "package": "pnpm build && electron-builder build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heychenfq/electron-quick-start.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heychenfq/electron-quick-start/issues"
  },
  "homepage": "https://github.com/heychenfq/electron-quick-start#readme",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@vitejs/plugin-react": "^1.3.1",
    "@electron-tools/ioc": "^1.0.8",
    "electron": "^18.0.2",
    "electron-builder": "^23.0.3",
    "esbuild": "^0.14.38",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "vite": "^2.9.5",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.1"
  }
}
