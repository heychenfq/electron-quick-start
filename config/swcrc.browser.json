{
  "jsc": {
    "parser": {
      "syntax": "typescript",
			"tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
			"legacyDecorator": true,
      "decoratorMetadata": true,
      "react": {
        "runtime": "automatic"
      }
		},
    "target": "es2016",
    "loose": false,
    "externalHelpers": true,
    "keepClassNames": true,
		"baseUrl": "../src"
  }
}